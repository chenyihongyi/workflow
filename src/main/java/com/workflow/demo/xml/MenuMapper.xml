<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.workflow.demo.dao.MenuMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.workflow.demo.entity.system.Menu">
		<id column="MENU_ID" property="menuId" />
		<result column="MENU_NAME" property="menuName" />
		<result column="MENU_URL" property="menuUrl" />
		<result column="PARENT_ID" property="parentId" />
		<result column="MENU_ORDER" property="menuOrder" />
		<result column="MENU_ICON" property="menuIcon" />
		<result column="MENU_TYPE" property="menuType" />
		<result column="MENU_STATE" property="menuState" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        MENU_ID AS menuId, MENU_NAME AS menuName, MENU_URL AS menuUrl, PARENT_ID AS parentId, MENU_ORDER AS menuOrder, MENU_ICON AS menuIcon, MENU_TYPE AS menuType, MENU_STATE AS menuState
    </sql>

</mapper>
